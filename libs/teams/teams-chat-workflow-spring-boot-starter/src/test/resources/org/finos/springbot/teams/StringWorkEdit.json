{
  "$schema" : "http://adaptivecards.io/schemas/adaptive-card.json",
  "version" : "1.3",
  "type" : "AdaptiveCard",
  "body" : [ {
    "type" : "Container",
    "style" : "emphasis",
    "items" : [ {
      "type" : "Input.Text",
      "value" : "${if(form.s,string(form.s),'')}",
      "id" : "s",
      "label" : "S"
    } ]
  }, {
    "type" : "ActionSet",
    "actions" : [ {
      "type" : "Action.Submit",
      "title" : "${text}",
      "id" : "${name}",
      "$data" : "${buttons.contents}"
    } ]
  } ]
}