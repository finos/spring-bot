{
  "$schema" : "http://adaptivecards.io/schemas/adaptive-card.json",
  "version" : "1.3",
  "type" : "AdaptiveCard",
  "body" : [ {
    "type" : "Container",
    "style" : "emphasis",
    "items" : [ {
      "type" : "Input.Toggle",
      "value" : "${form.s}",
      "id" : "form-field:s",
      "title" : "S"
    } ]
  }, {
    "type" : "ActionSet",
    "actions" : [ {
      "type" : "Action.Submit",
      "title" : "${text}",
      "id" : "${name}",
      "associatedInputs" : "auto",
      "data" : {
        "action" : "${name}",
        "form" : "${$root.formid}",
        "storageId" : "${$root.storageId}"
      },
      "$data" : "${buttons.contents}"
    } ]
  } ]
}